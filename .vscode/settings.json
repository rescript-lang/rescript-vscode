{
  "editor.insertSpaces": false,
  "tslint.enable": true,
  "typescript.tsc.autoDetect": "off",
  "typescript.preferences.quoteStyle": "single",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "ocaml.sandbox": {
    "kind": "opam",
    "switch": "${workspaceFolder:rescript-vscode}"
  }
}
